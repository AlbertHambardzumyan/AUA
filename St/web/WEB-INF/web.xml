<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>store3</servlet-name>
        <servlet-class>store.menu.MenuListItemsSetup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store3</servlet-name>
        <url-pattern>/MenuListItemsSetup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store4</servlet-name>
        <servlet-class>store.menu.SubMenuListItemsSetup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store4</servlet-name>
        <url-pattern>/SubMenuListItemsSetup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store5</servlet-name>
        <servlet-class>store.product.ProductItemsSetup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store5</servlet-name>
        <url-pattern>/ProductItemsSetup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store6</servlet-name>
        <servlet-class>store.cart.ShoppingCartSetter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store6</servlet-name>
        <url-pattern>/ShoppingCartSetter</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store7</servlet-name>
        <servlet-class>store.login.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store7</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store8</servlet-name>
        <servlet-class>store.register.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store8</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store9</servlet-name>
        <servlet-class>store.pictures.MainPicturesSetter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store9</servlet-name>
        <url-pattern>/MainPicturesSetter</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>store10</servlet-name>
        <servlet-class>store.authentication.AuthenticationChecker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>store10</servlet-name>
        <url-pattern>/AuthenticationChecker</url-pattern>
    </servlet-mapping>
</web-app>